{
  "target": "bmv2",
  "p4info": "build/networkflow.p4.p4info.txt",
  "bmv2_json": "build/networkflow.json",
  "table_entries": [
    {
      "table": "MyEgress.update_int",
      "default_action": true,
      "action_name": "MyEgress.update_timestamps"
    },
    {
      "table": "MyEgress.update_int",
      "default_action": true,
      "action_name": "MyEgress.update_queue"
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4.dstAddr": ["10.0.1.0", 24]
      },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": {
        "dstAddr": "00:00:00:00:01:00",
        "port": 1
      }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4.dstAddr": ["10.0.2.0", 24]
      },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": {
        "dstAddr": "00:00:00:00:02:00",
        "port": 2
      }
    }
  ]
}