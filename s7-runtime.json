{
    "target": "bmv2",
    "p4info": "build/networkflow.p4.p4info.txt",
    "bmv2_json": "build/networkflow.json",
    "table_entries": [
      {
        "table": "Egress.update_int",
        "default_action": true,
        "action_name": "Egress.update_timestamps"
      },
      {
        "table": "Egress.update_int",
        "default_action": true,
        "action_name": "Egress.update_queue"
      },
      {
        "table": "Ingress.ipv4_lpm",
        "match": {
          "hdr.ipv4.dstAddr": ["10.0.5.0", 24]
        },
        "action_name": "Ingress.ipv4_forward",
        "action_params": {
          "dstAddr": "00:00:00:00:05:00",
          "port": 1
        }
      },
      {
        "table": "Ingress.ipv4_lpm",
        "match": {
          "hdr.ipv4.dstAddr": ["10.0.8.0", 24]
        },
        "action_name": "Ingress.ipv4_forward",
        "action_params": {
          "dstAddr": "00:00:00:00:08:00",
          "port": 2
        }
      }
    ]
  }